proc expressions()
{
	10
	20
	1 + 2 + 3 + 4
	1 + 2 * 3
	1 * 2 + 3
	2 == 3 && 4 == 5 || 6 == 7 && 8 == 9
	2 & 3 | 6 & 7
	2 & 3 ^ 6 & 7
	1 << 2 + 2
	1 | 2 ^ 3 | 4 ^ 5
	a
	a + b
}
======
proc expressions()
{
	10
	20
	(((1 + 2) + 3) + 4)
	(1 + (2 * 3))
	((1 * 2) + 3)
	(((2 == 3) && (4 == 5)) || ((6 == 7) && (8 == 9)))
	((2 & 3) | (6 & 7))
	((2 & 3) ^ (6 & 7))
	((1 << 2) + 2)
	((((1 | 2) ^ 3) | 4) ^ 5)
	a
	(a + b)
}
